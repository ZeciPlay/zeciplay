{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js", 
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.gif",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/android/dm7",
      "dest": "/Android/Dm7Android.html"
    },
    {
      "src": "/mobile",
      "dest": "/htmls/Mobile.html"
    },
    {
      "src": "/port",
      "dest": "/htmls/port.html"
    },
    {
      "src": "/gameloft",
      "dest": "/htmls/gameloft.html"
    },
    {
      "src": "/webgames",
      "dest": "/htmls/webgames.html"
    },
    {
      "src": "/undertale",
      "dest": "/undertale/unindex.html"
    },
    {
      "src": "/ddlc",
      "dest": "/DDLC/ddlc.html"
    },
    {
      "src": "/memorial", 
      "dest": "/M/memorial.html"
    },
    {
      "src": "/adventure-time-card-wars",
      "dest": "/jogos/ATCW/ATCard.html"
    },
    {
      "src": "/assassins-creed-pirates",
      "dest": "/jogos/ACP/ACP.html"
    },
    {
      "src": "/balatro",
      "dest": "/jogos/Bal/Bal.html"
    },
    {
      "src": "/terraria",
      "dest": "/jogos/Terraria/terindex.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|xml|txt))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}