{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/teste-agora",
      "dest": "/Android/Dm7Android.html"
    },
    {
      "src": "/android/dm7",
      "dest": "/Android/Dm7Android.html"
    },
    {
      "src": "/mobile",
      "dest": "/htmls/Mobile.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}